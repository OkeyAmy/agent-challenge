# Example environment variables for local development
# Copy this file to .env and replace the placeholder values as needed

# Ollama model name to use (optional)
MODEL_NAME_AT_ENDPOINT="qwen2.5:1.5b"

# Base URL of your Ollama (or other) API endpoint
API_BASE_URL="https://your-ollama-endpoint.example.com/api"

# Google Generative AI API key (required if you want to use Gemini fallback)
GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY_HERE"

# RapidAPI key for Amazon product search (optional, enables real product data)
RAPIDAPI_KEY="YOUR_RAPIDAPI_KEY_HERE"